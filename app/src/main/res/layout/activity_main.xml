<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="NO UI.  See logs for activity"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/utensil_0"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toTopOf="@+id/developer_4"
        app:layout_constraintEnd_toStartOf="@+id/developer_0"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/utensil" />

    <ImageView
        android:id="@+id/developer_0"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="22dp"
        app:layout_constraintEnd_toStartOf="@+id/utensil_1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/utensil_0"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/food" />

    <ImageView
        android:id="@+id/utensil_1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toTopOf="@+id/developer_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/developer_0"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/utensil" />

    <ImageView
        android:id="@+id/developer_3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/utensil_3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/utensil_4"
        app:srcCompat="@drawable/food" />

    <ImageView
        android:id="@+id/utensil_3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginBottom="22dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/developer_2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/developer_3"
        app:srcCompat="@drawable/utensil" />

    <ImageView
        android:id="@+id/developer_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/utensil_3"
        app:layout_constraintTop_toBottomOf="@+id/utensil_2"
        app:srcCompat="@drawable/food" />

    <ImageView
        android:id="@+id/utensil_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginRight="20dp"
        app:layout_constraintBottom_toTopOf="@+id/developer_2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/developer_1"
        app:srcCompat="@drawable/utensil" />

    <ImageView
        android:id="@+id/developer_1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginRight="20dp"
        app:layout_constraintBottom_toTopOf="@+id/utensil_2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/utensil_1"
        app:srcCompat="@drawable/food" />

    <ImageView
        android:id="@+id/utensil_4"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintBottom_toTopOf="@+id/developer_3"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/developer_4"
        app:srcCompat="@drawable/utensil" />

    <ImageView
        android:id="@+id/developer_4"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintBottom_toTopOf="@+id/utensil_4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/utensil_0"
        app:srcCompat="@drawable/food" />

</android.support.constraint.ConstraintLayout>